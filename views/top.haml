.navbar.navbar-inverse
  .navbar-inner
    .container-fluid
      %a.brand{:href => "#{@path_prefix}/"}
        実況アプリ
      .nav-collapse.collapse
        %ul.nav
.row-fluid
  .span6
    %h1 実況アプリ
    %br
    %table{:class => "table table-hover"}
      %thead
        %tr
          %th 実況タイトル　　　　　　  　　
          %th 時間　　　　　　　　
          %th 発言数
      %tbody
        - @channels.each do |channel|
          %tr{:class => "success"}
            %td
              %a{:href => "#{@path_prefix}/channel/detail/" + "#{channel.id}"} #{channel.title}
            %td #{channel.elapsetime}
            %td #{channel.rows}

