.navbar.navbar-inverse
  .navbar-inner
    .container-fluid
      %a.brand{:href => "#{@path_prefix}/"}
        実況アプリ
      .nav-collapse.collapse
        %ul.nav
          %li
            %a{:href => "#{@path_prefix}/jikkyoulist/"}
              実況一覧
.row-fluid
  .span6
    %h1 #{@channel.title}
    %h2 実況一覧
    %table{:class => "table table-hover"}
      %thead
        %tr
          %th 名前　　　　　　　　　　　　　　　　　　　　　
          %th 上へ
          %th 下へ
      %tbody
        - @contents.each_with_index do |content,i|
          %tr{:class => "info"}
            %td{:align =>"center"}
              =content.tweets
            %td
              =content.tweetstime
            %td
              =content.value
.span6